    * {
        font-family: 'Noto Sans TC', sans-serif;
    }
    
    img {
        width: 100%;
    }
    /* header */
    
    header {
        position: fixed;
        top: 0;
        right: 0;
        left: 0;
        z-index: 1030;
        background: #fff;
        padding: 2rem 0;
        box-shadow: 0px 1px 7px rgba(0, 0, 0, 0.1);
    }
    
    header .container {
        display: flex;
        align-items: flex-end;
        justify-content: space-between;
    }
    
    .logo {
        position: relative;
    }
    
    .logo::before {
        content: '';
        position: absolute;
    }
    
    .right {
        display: flex;
        align-items: flex-end;
    }
    
    .right h3 {
        font-family: 'Noto Sans TC', sans-serif;
        padding-bottom: .4rem;
        border-bottom: 3px solid #8c001a;
        color: #8c001a;
        margin-right: 1.5rem;
    }
    
    .desktop_search {
        display: flex;
        align-items: flex-end;
        position: relative;
    }
    
    .right .search .btn {
        width: 40px;
        height: 27px;
        border: none;
        background: none;
        font-size: 18px;
        font-weight: bolder;
        color: #8c001a;
        transform: scaleX(-1);
        cursor: pointer;
    }
    
    .desktop_search span {
        content: url(/blog/images/search.png);
        position: absolute;
        top: 3px;
        left: 9px;
        pointer-events: none;
        cursor: pointer;
    }
    
    .search input:last-of-type {
        width: 200px;
        height: 27.39px;
        border: 1px solid #8c001a;
        border-left: 3px solid #8c001a;
        border-right: 3px solid #8c001a;
        outline: none;
        padding: 0 .3rem;
        letter-spacing: 2px;
    }
    
    .desktop_search input::placeholder {
        color: #bababa;
    }
    
    .tab_search_field {
        display: none;
    }
    /* 內容 */
    
    .bolg_main {
        margin: 9rem 0 6rem;
    }
    
    .bolg_main .container {
        display: flex;
        align-items: flex-start;
        justify-content: space-between;
    }
    
    .main_left {
        width: 65%;
    }
    
    .main_right {
        width: 30%;
        position: relative;
    }
    
    .main_left .label {
        display: flex;
        align-items: center;
    }
    
    .main_left .label span {
        padding: .15rem .5rem;
        background: #8c001a;
        border-radius: 5px;
        color: #fff;
        letter-spacing: 2px;
        text-indent: 2px;
        margin-right: .5rem;
    }
    
    .main_left .title {
        border-bottom: 1px solid #959595;
        padding: .6rem 0;
        display: flex;
        align-items: center;
        justify-content: space-between;
    }
    
    hr {
        margin: .3rem 0 3rem;
    }
    
    .title_box_left>div span {
        font-size: 15px;
        margin-right: 3rem;
    }
    
    .title_box_left>img {
        width: 26px;
        height: 26px;
        object-fit: cover;
        border-radius: 50%;
        margin-right: .8rem;
    }
    
    .title_box_left .author {
        display: flex;
        align-items: center;
    }
    
    .title_box_left {
        display: flex;
        align-items: center;
        justify-content: flex-start;
    }
    
    .title_box_right {
        display: flex;
        align-items: center;
        justify-content: flex-end;
    }
    
    .title_box_right a {
        margin-right: .5rem;
    }
    
    .title_box_right a:last-of-type {
        margin-right: 0;
    }
    
    .title_box_right button {
        background: none;
        border: none;
        margin-left: .5rem;
    }
    
    .line:hover img {
        content: url(/blog/images/BLOG_line-1.png);
    }
    
    .fb:hover img {
        content: url(/blog/images/BLOG_fb-1.png);
    }
    
    .link:hover img {
        content: url(/blog/images/BLOG_link-1.png);
    }
    /* .main_left h1 {
        font-size: 40px;
    } */
    /* 文章 */
    
    .article {
        line-height: 1.5rem;
    }
    
    .article img {
        display: block;
    }
    
    .article .txt {
        border-right: 5px solid #959595;
        padding-right: .5rem;
        padding-top: .8rem;
    }
    
    .article .txt {
        text-align: right;
        color: #959595;
    }
    
    .photo_txt {
        font-family: 微軟正黑體;
        font-size: 15px;
        font-style: italic;
        color: #525252;
        padding: 3rem;
        width: 100%;
        background: #fef7f9;
        margin-top: 2rem;
        line-height: 2rem;
        letter-spacing: 2px;
    }
    
    .photo_txt .container {
        width: 100%;
        padding: 2rem 5rem;
        background-image: linear-gradient( -30deg, #fef7f9 15.7%, rgba(0, 0, 0, 0.5) 15%, transparent 16%, transparent 85.7%, rgba(0, 0, 0, 0.5) 86%, #fef7f9 86%);
    }
    
    .photo_txt p {
        padding: 2rem 0;
        text-align: justify;
    }
    
    .article h3 {
        font-size: 22px;
        margin-top: 2rem;
        display: flex;
        align-items: flex-end;
    }
    
    .article h3 i,
    .hot p i {
        font-size: 10px;
        color: #8c001a;
        margin-right: .3rem;
    }
    
    .article_txt p {
        font-family: 微軟正黑體;
        font-size: 16px;
        line-height: 2rem;
        margin-top: 2rem;
        text-align: justify;
    }
    
    .article_img_two {
        margin-top: 2rem;
        display: flex;
        align-items: center;
        justify-content: center;
    }
    
    .article_img_box {
        width: 49%;
        margin: 0 0.5%;
    }
    
    .article_img_box img {
        max-width: 100%;
        height: 442px;
        object-fit: cover;
    }
    /* ig */
    
    .instagram-media {
        margin: 2rem auto 0 !important;
    }
    /* youtube */
    
    .video_container {
        position: relative;
        width: 100%;
        height: 0;
        padding-bottom: 56.25%;
        margin-top: 3rem;
    }
    
    .video {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
    }
    /* 熱門推薦 */
    
    .hot {
        margin-top: 3.5rem;
    }
    
    .hot p {
        font-size: 18px;
        font-weight: bolder;
        background: #fdedf0;
        padding: .5rem 1rem;
    }
    
    .hot ul li {
        font-size: 16px;
        padding: 1rem .5rem;
        border-bottom: 1px dashed #959595;
    }
    
    .hot ul li:last-of-type {
        border-bottom: none;
    }
    /* 標籤 */
    
    .down_label {
        margin-top: 2rem;
        color: #8f8f8f;
        padding-bottom: .8rem;
        border-bottom: 2px solid #959595;
        display: flex;
        align-items: center;
    }
    
    .down_label a {
        border: 1px solid #8f8f8f;
        padding: .1rem;
        margin-left: .5rem;
        color: #8f8f8f;
    }
    
    .down_label a:hover {
        color: #fff;
        background: #8f8f8f;
    }
    /* 上、下一篇 */
    
    .article_next {
        margin: .3rem 0;
        border-top: 0.75px solid #959595;
        border-bottom: 0.75px solid #959595;
        padding: 1rem;
        display: flex;
        align-items: center;
        justify-content: space-between;
    }
    
    .article_title {
        width: 100%;
    }
    
    .article_next .article_title a {
        width: 15em;
        font-size: 16px;
        margin: 0 auto;
        line-height: 1.5rem;
        word-break: normal;
        display: -webkit-box;
        -webkit-line-clamp: 2;
        -webkit-box-orient: vertical;
        overflow: hidden;
        text-overflow: ellipsis;
    }
    
    .article_title:first-of-type {
        border-right: thin solid #959595;
        /* background-image: linear-gradient(-70deg, #fff 4%, #b5b5b5 5%, transparent 4%); */
    }
    
    .article_title:first-of-type a {
        margin-left: 0;
    }
    
    .article_title:last-of-type a {
        margin-right: 0;
        text-align: right;
    }
    
    .article_next i {
        font-size: 18px;
        color: #9a0012;
    }
    
    .article_next .fa-chevron-left {
        padding-right: 1rem;
    }
    
    .article_next .fa-chevron-right {
        padding-left: 1rem;
    }
    /* 作者介紹 */
    
    .introduction {
        border-top: 2px solid #959595;
        border-bottom: 1px dashed #959595;
        padding: 1.5rem 2rem;
        display: flex;
        align-items: center;
    }
    
    .introduction>img {
        width: 140px;
        height: 140px;
        border-radius: 50%;
        padding: .3rem;
        margin-right: 2rem;
        border: 1px solid #959595;
    }
    
    .introduction .author_box {
        display: flex;
        align-items: center;
    }
    
    .introduction .author_box p {
        margin-right: .5rem;
    }
    
    .introduction .author_box .line {
        margin: 0 .5rem;
    }
    
    .about>p {
        line-height: 1.5rem;
        color: #959595;
        margin-top: .5rem;
        text-align: justify;
    }
    
    .author_box p {
        font-size: 18px;
        color: #000;
    }
    
    .down_hr {
        margin: .3rem 0;
        border: 1px solid #959595;
    }
    
    @media (max-width:800px) {
        header {
            background: #8c001a;
            box-shadow: none;
            padding: 10px 0;
        }
        header .container {
            position: relative;
        }
        .logo img {
            content: url(../images/BLOG_logo-2.png);
        }
        .right h3 {
            border-bottom: 3px solid #fff;
            color: #fff;
            margin-right: 0;
        }
        .right .search input.btn {
            color: #fff;
        }
        .desktop_search {
            display: none;
        }
        .tab_search_field {
            display: flex;
            width: 100%;
            position: fixed;
            top: -25px;
            background: #d1929d;
            padding: .15rem;
            transition: .5s;
            z-index: 1;
        }
        #Header1_Panel1 {
            display: flex;
        }
        /* --------------- headroom --------------- */
        .header--fixed {
            position: absolute;
            top: -85px;
            width: 100%;
        }
        .header--fixed.top {
            transition: none;
            transform: translateY(0);
        }
        .header--fixed.not-top {
            position: fixed;
            transform: translateY(-100%);
        }
        .header--fixed.slideDown.not-top {
            transition: transform 0.3s ease-in-out;
            transform: translateY(160px);
        }
        .header--fixed.slideDown.top {
            transition: transform 0.3s ease-in-out;
            position: fixed;
        }
        .header--fixed.slideUp.not-top {
            transition: transform 0.3s ease-in-out;
            transform: translateY(100px);
        }
        .header--fixed.slideUp.top {
            transform: translateY(-100%);
            position: absolute;
        }
        .tab_search_field {
            width: 100%;
        }
        .tab_search_field input:first-of-type {
            font-size: 1rem;
            width: 100%;
            outline: none;
            border: none;
            padding: .15rem;
            letter-spacing: 2px;
        }
        .tab_search_field input:last-of-type {
            font-size: 1rem;
            padding: .8rem;
            letter-spacing: 2px;
            text-indent: 2px;
            background: #d1929d;
            border: none;
            color: #fff;
        }
        .tab_search_field input::placeholder {
            color: #bababa;
        }
        .article_img_two {
            flex-wrap: wrap;
            align-items: center;
            justify-content: flex-start;
        }
        .article_img_box:first-of-type {
            margin: 0 0 1.5rem;
        }
        .article_img_box {
            width: auto;
        }
        .article_img_box img {
            height: auto;
        }
    }
    
    @media (max-width:600px) {
        .main_left .title {
            align-items: flex-end;
        }
        .title_box_left .date {
            margin-bottom: .5rem;
        }
    }
    
    @media (max-width:500px) {
        .logo img {
            width: 80%;
            height: auto;
            object-fit: contain;
        }
        .header--fixed {
            top: -96px;
        }
        .header--fixed.top {
            transform: translateY(0);
        }
        .header--fixed.slideDown.not-top {
            transform: translateY(160px);
        }
        .header--fixed.slideUp.not-top {
            transform: translateY(113px);
        }
        .title_box_left {
            width: 60%;
        }
    }
    
    @media (max-width:414px) {
        .logo img {
            content: url(../images/BLOG_logo-2-1.png);
        }
    }
    
    @media(max-width:350px) {
        .title_box_left {
            width: 70%;
        }
    }
    
    @media (max-width: 320px) {
        .header--fixed {
            top: -95px;
        }
    }
    /* ----------------------- 右邊內容 ----------------------- */
    /* 熱門精選 */
    
    .featured h2 {
        color: #8c001a;
        border-bottom: 6px solid #8c001a;
        padding: 0 0 .5rem 1rem;
        letter-spacing: 3px;
        position: relative;
    }
    
    .featured h2:before {
        content: url(/blog/images/斜線.png);
        position: absolute;
        right: 33px;
    }
    
    .featured_big_box {
        padding: .5rem 0;
        border-bottom: 1px solid #b5b5b5;
    }
    
    .featured_big_box:last-of-type {
        border-bottom: none;
    }
    
    .featured_box {
        background: #fef7f9;
        padding: 12px 1.5rem;
        display: flex;
        align-items: center;
    }
    
    .featured_box img {
        width: 86px;
        height: 86px;
        object-fit: cover;
        border-radius: 5px;
        margin-right: 1rem;
    }
    
    .img_txt {
        height: 72px;
        display: flex;
        flex-direction: column;
        justify-content: space-around;
    }
    
    .img_txt p {
        font-size: 16px;
    }
    
    .img_txt .title {
        line-height: 1.35rem;
        word-break: normal;
        display: -webkit-box;
        -webkit-line-clamp: 2;
        -webkit-box-orient: vertical;
        overflow: hidden;
        text-overflow: ellipsis;
        letter-spacing: 1px;
    }
    
    .img_txt p:last-of-type {
        color: #8f8f8f;
        font-size: 14px;
        font-weight: 300;
    }
    /* 目錄 */
    
    .list {
        margin-top: 3rem;
    }
    
    .list h2 {
        color: #000;
        border-bottom: 6px solid #000;
        padding: 0 0 .5rem 1rem;
        letter-spacing: 3px;
        position: relative;
    }
    
    .list h2:before {
        content: url(/blog/images/斜線-2.png);
        position: absolute;
        right: 33px;
    }
    
    .list>div {
        width: 90%;
        margin: 0 auto;
        border-left: 2px solid #959595;
    }
    
    .list ul {
        padding: 0 0 .5rem 1rem;
    }
    
    .list ul li {
        padding: 1rem .5rem;
        border-bottom: 0.75px solid #b5b5b5;
    }
    
    .list ul li:last-of-type {
        border-bottom: none;
    }
    
    .list ul li a {
        font-size: 15px;
        display: flex;
        align-items: center;
    }
    
    .list ul li a i {
        font-size: 2px;
        margin-right: .3rem;
    }
    
    .is-inview {
        color: #8c001a;
        background: none;
    }
    
    @media (max-width:1880px) {
        .featured h2:before,
        .list h2:before {
            right: -54px;
            width: 80%;
        }
    }
    
    @media (max-width:1280px) {
        .featured h2:before,
        .list h2:before {
            display: none;
        }
    }
    
    @media (max-width:1067px) {
        .bolg_main .container {
            flex-wrap: wrap;
        }
        .main_left,
        .main_right,
        .featured h2 {
            width: 100%;
        }
        .introduction {
            border-bottom: none;
        }
        .featured {
            margin-top: 2rem;
        }
        .down_hr,
        .list {
            display: none;
        }
        .bolg_main {
            margin: 9rem 0 3rem;
        }
    }
    
    @media (max-width:800px) {
        .bolg_main {
            margin: 10rem 0 3rem;
        }
        .photo_txt .container {
            padding: 2rem 3rem;
        }
    }
    
    @media (max-width:640px) {
        .article_next .article_title a {
            width: 10em;
        }
    }
    
    @media (max-width:600px) {
        .introduction {
            flex-wrap: wrap;
        }
        .introduction>img {
            margin: 0 auto 1rem;
        }
        .introduction .about {
            width: 100%;
        }
    }
    
    @media (max-width:453px) {
        .article_next .article_title a {
            width: 8em;
        }
        .photo_txt .container {
            padding: 1.5rem 1rem;
        }
    }
    
    @media (max-width:365px) {
        iframe {
            min-width: 287px !important;
        }
        .introduction {
            padding: 1.5rem .9rem;
        }
    }
    
    @media (max-width:344px) {
        .article_next .article_title a {
            width: 5em;
        }
    }