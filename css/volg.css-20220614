/* ------------ tab_e ------------ */


/* 輪播圖 */

.tab_e {
  position: relative;
}

.e_top {
  height: 700px;
  position: relative;
}

.e_top>img {
  height: 100%;
  object-fit: cover;
}

.e_top_txt {
  width: 100%;
  position: absolute;
  top: 20%;
  display: flex;
  justify-content: center;
}

.left_box {
  display: flex;
}

.img_b {
  width: 282px;
  object-fit: cover;
}

.img_b img {
  width: 100%;
}

.title_a {
  font-family: 'Noto Sans TC', sans-serif;
  color: #fff;
  padding-left: 2rem;
  margin: 0 4.0625rem 0 9.25rem;
  position: relative;
}

.title_a::before {
  content: '';
  position: absolute;
  left: 0;
  top: 8px;
  width: 5px;
  height: 65px;
  background: #fff;
}

.title_b {
  color: #fff;
  margin: 0 0 2rem;
}

.narrate {
  font-size: 18px;
  color: #fff;
  line-height: 1.5;
  margin-bottom: 2rem;
}

.listen {
  font-family: 'Noto Sans TC', sans-serif;
  font-weight: bold;
  color: #fff;
  border: 2px solid #fff;
  border-radius: 50px;
  padding: .2rem 1.5rem;
  transition: .5s;
}

.listen:hover {
  text-decoration: none;
  color: #2e4787;
  background: #fff;
  transition: .5s;
}

.tab_e .owl-nav {
  width: 100%;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  margin: 0;
}

.tab_e .owl-prev {
  position: absolute;
  left: 40px;
}

.tab_e .owl-next {
  position: absolute;
  right: 40px;
}

.tab_e .owl-nav [class*=owl-]:hover {
  background: none;
}

.tab_e .owl-prev span {
  content: url(../images/e_prev.png);
}

.tab_e .owl-next span {
  content: url(../images/e_next.png);
}


/* 最新上線 */

.e_down {
  background: #2e4787;
}

.e_down .container {
  margin-top: -14rem;
  z-index: 1;
}

.e_down h3 {
  font-size: 25px;
  color: #fff;
  text-align: center;
  padding-bottom: 5rem;
  position: relative;
}

.e_down h3::before {
  content: url(../images/h3_icon.png);
  position: absolute;
  left: 50%;
  bottom: 25px;
  transform: translateX(-50%);
}

.e_down_box .down_img {
  overflow: hidden;
  position: relative;
  transition: .5s;
}

.e_down_box .down_img img {
  transition: .5s;
}

.e_down_box .down_img::before {
  content: '';
  position: absolute;
  left: 0;
  bottom: 0;
  width: 65px;
  height: 9px;
  background: #c9c9c9;
  transition: .5s;
  z-index: 1;
}

.e_down_box .down_img {
  border: thin solid #c9c9c9;
}

.e_down_box a:hover {
  text-decoration: none;
}

.e_down_box a:hover .down_img {
  border: thin solid #8c001a;
  transition: .5s;
}

.e_down_box a:hover .down_img img {
  transform: scale(1.1);
  filter: brightness(.5);
  transition: .5s;
}

.e_down_box a:hover .down_img::before {
  background: #8c001a;
  transition: .5s;
}

.down_img span {
  font-size: 18px;
  font-weight: bold;
  color: transparent;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  transition: .5s;
}

.e_down_box a:hover .down_img span {
  color: #fff;
  transition: .5s;
}

.down_txt {
  font-family: 'Noto Sans TC', sans-serif;
  font-size: 20px;
  color: #fff;
  line-height: 1.5;
  margin-top: 1rem;
}

.e_down_box .owl-dots .owl-dot span {
  background: transparent;
  border: 1px solid #fff;
}

.e_down_box .owl-dots .owl-dot.active span,
.e_down_box .owl-dots .owl-dot:hover span {
  background: #fff;
}

