  font-size: 16px;
  cursor: pointer;
  display: flex;
  align-items: center;
  justify-content: center;
}

.page span:last-of-type {
  margin-right: 0;
}

.page span.active {
  width: 40px;
  height: 40px;
  border-radius: 100%;
  background: #959595;
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
}


