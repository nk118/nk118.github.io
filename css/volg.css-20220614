/* -------------- VOLG -------------- */

.os-scrollbar-vertical {
  display: none;
}

.main-banner #h1 {
  width: 100%;
  text-align: center;
  font-family: 'Noto Sans TC', sans-serif;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  color: #fff;
  font-size: 2.3rem;
  letter-spacing: 5px;
  text-shadow: 1px 2px 5px rgb(0 0 0 / 50%);
  margin: 5px 0;
}

.VOLG {
  padding: 30px 0 0 0;
}

.VOLG-nav-tab {
  height: 4rem;
}

.VOLG-nav-tab .container {
  width: 60%;
  /* display: flex; */
  height: 100%;
}


/* .VOLG-nav-tab .container>button a {
        margin-bottom: .6rem;
    } */

.VOLG-nav-tab .container button {
  border: none;
  background: none;
  white-space: nowrap;
}

.VOLG-nav-tab .container button a {
  font-family: 'Noto Sans TC', sans-serif;
  font-size: 1.1rem;
  border-left: 1px solid #959595;
  text-decoration: none;
}

.VOLG-nav-tab .tab {
  width: 100%;
  height: 68px;
  list-style: none;
  padding: 0;
  display: flex;
  justify-content: space-between;
  white-space: nowrap;
  overflow-x: auto;
  overflow-y: hidden;
  line-height: 4rem;
}

.VOLG-nav-tab .tab button {
  font-family: 'Noto Sans TC', sans-serif;
  font-size: 1.4rem;
  cursor: pointer;
  display: inline-block;
  background: white;
  border: none;
  position: relative;
}

.VOLG-nav-tab .tab button:last-of-type {
  border-right: 1px solid #959595;
}

.VOLG-nav-tab .container button a,
.VOLG-nav-tab .tab button a {
  display: block;
  /* line-height: 50px; */
  padding: 0 3.6rem;
}

.VOLG-nav-tab .container button a:hover,
.VOLG-nav-tab .tab button a:hover,
.VOLG-nav-tab .tab button.active a {
  color: #8c001a;
  font-weight: bolder;
  text-decoration: none;
}

.VOLG-nav-tab .tab button:focus {
  outline: 0;
}


/* .VOLG-nav-tab .container button::before, */

.VOLG-nav-tab .tab button::before {
  content: '';
  width: 101%;
  height: 15px;
  position: absolute;
  top: 16px;
  left: 1.9px;
  pointer-events: none;
}


/* .VOLG-nav-tab .container button:hover:before, */

.VOLG-nav-tab .tab button:hover::before,
.VOLG-nav-tab .tab button.active::before {
  border-bottom: 5px solid #8c001a;
}

.VOLG-nav-tab .tab button:hover {
  color: #8c001a;
}

.VOLG-nav-tab .tab button::after {
  content: url(../images/20_triangle.png);
  position: absolute;
  top: 23px;
  left: 47%;
  display: none;
  letter-spacing: 0;
  text-indent: 0;
}

.VOLG-nav-tab .tab button:hover::after,
.VOLG-nav-tab .tab button.active::after {
  display: block;
}


/* 最新文章 */

.volg-owl-box {
  background: #f5f5f5;
}

.tab_inner .container {
  padding: 2rem 0 3rem;
}

.volg-owl-item {
  display: flex;
  align-items: center;
}

.volg-owl-item:hover {
  text-decoration: none;
}

.volg-owl-item .img {
  width: 75%;
  margin-right: 2rem;
}

.volg-owl-item .img img {
  width: 100%;
  padding: .5rem;
  border: 1px solid #acacac;
}

.volg-owl-title {
  font-family: 'Noto Sans TC', sans-serif;
  padding-left: 1rem;
  font-size: 2em;
  line-height: normal;
  position: relative;
}

.volg-owl-title::before {
  content: '';
  width: 100%;
  height: 24px;
  border-left: 5px solid #8c001a;
  position: absolute;
  top: 9px;
  left: 0;
}

.volg-owl-text {
  width: 40%;
  display: flex;
  flex-direction: column;
  /* justify-content: space-between; */
}

.volg-owl-text .label_a span {
  font-size: 16px;
  color: #959595;
}

.volg-owl-text .label {
  font-family: 'Noto Sans TC', sans-serif;
  font-size: 16px;
  font-weight: bolder;
  color: #0090ff;
  padding: .2rem;
  border: 1px solid #0090ff;
  display: inline-block;
  margin: 1rem 0;
  border-radius: 0;
}

.volg-owl-text h3 {
  font-family: 'Noto Sans TC', sans-serif;
  font-size: 18px;
  margin-bottom: 1rem;
  line-height: normal;
  margin-top: 0;
}

.volg-owl-item .txt {
  font-weight: bolder;
  line-height: 1.5rem;
  text-align: justify;
}

.more {
  display: flex;
  align-items: center;
  justify-content: flex-end;
  margin-top: 2rem;
}

.more img {
  width: auto !important;
  margin-right: .5rem;
}

.more p {
  font-family: 'Noto Sans TC', sans-serif;
  font-weight: bolder;
  color: #959595;
  white-space: nowrap;
}

.volg-owl-box .volg-owl {
  position: relative;
}

.volg-owl-box .owl-dots {
  position: absolute;
  top: -13px;
  left: -25px;
  display: flex;
  flex-direction: column;
}

.volg-owl-box .owl-theme .owl-dots .owl-dot span {
  width: 5px;
  height: 26.91px;
  border-radius: 0;
  border: 1px solid #959595;
  background: none;
  margin: 3px 7px;
}

.volg-owl-box .owl-theme .owl-dots .owl-dot.active span {
  background: #959595;
}


/* 熱門推薦 */

.hot_box {
  width: 88%;
  margin: 0 auto;
  border-collapse: collapse;
  table-layout: fixed;
}

.hot_box tr {
  border-bottom: 1px solid #959595;
}

.hot_box tr:last-of-type {
  border: none;
}

.hot_box tr td {
  padding: .6rem .2rem;
}

.hot_box tr td a {
  font-family: 'Noto Sans TC', sans-serif;
  font-weight: bolder;
  font-size: 18px;
  width: 90%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  text-align: left;
  display: block;
}

.hot_box tr td a:hover {
  text-decoration: none;
  color: #8c001a;
}

.hot_box tr td a span {
  display: inline-block;
  text-align: center;
  font-family: Arial, Helvetica, sans-serif;
  font-style: italic;
  color: #757575;
  width: 20px;
  height: 20px;
  border-radius: 100%;
  background: #ffd5dd;
  margin-right: 1rem;
}


/* 精選主題 bootstrap */


/* search */

.desktop_search {
  display: flex;
  align-items: center;
  justify-content: flex-end;
  position: relative;
}

.right .search .btn {
  width: 40px;
  height: 27px;
  border: none;
  background: none;
  font-size: 18px;
  font-weight: bolder;
  color: #8c001a;
  transform: scaleX(-1);
  cursor: pointer;
}

.desktop_search span {
  content: url(../images/search.png);
  /* content: url(/blog/images/search.png); */
  position: absolute;
  top: 3px;
  right: 228px;
  pointer-events: none;
  cursor: pointer;
}

.desktop_search input:first-of-type {
  margin-right: .6rem;
  background: none;
  border: none;
  outline: none;
  box-shadow: none;
}

.desktop_search input:last-of-type {
  width: 200px;
  height: 27.39px;
  border: 1px solid #8c001a;
  border-left: 3px solid #8c001a;
  border-right: 3px solid #8c001a;
  outline: none;
  padding: 0 .3rem;
  letter-spacing: 2px;
  border-radius: 0;
  margin-bottom: 0;
}

.desktop_search input::placeholder {
  color: #bababa;
}

.well {
  padding: 19px 0;
}

.nav .dropdown-toggle .caret {
  border-top-color: transparent;
  border-bottom-color: transparent;
  position: relative;
}

.nav .dropdown-toggle:hover .caret,
.nav .dropdown-toggle:focus .caret {
  border-top-color: transparent;
  border-bottom-color: transparent;
}

.nav>li>a>img {
  margin-right: .2rem;
}

.nav .dropdown-toggle .caret::before {
  content: url(../images/tab_down.png);
  position: absolute;
  right: -11px;
  top: -15px;
}

.nav-pills.tab {
  display: none;
}

.nav-pills.desktop {
  border-top: 1px solid #959595;
  border-bottom: 1px solid #959595;
  font-size: 18px;
}

.nav-pills>li {
  padding: 1rem 0;
}

.nav-pills>li>a {
  height: 22px;
  /* font-family: 'Noto Sans TC', sans-serif; */
  padding: 0 2rem;
  border-left: 1px solid #959595;
  border-radius: 0;
  color: #757575;
  display: flex;
  align-items: center;
}

.nav-pills>li:nth-child(2) a {
  border-left: none;
  color: #535353;
  justify-content: space-between;
}

.nav-pills.desktop>li:first-of-type a:hover,
.nav-pills.desktop>li:nth-child(2) a:hover {
  color: #8c001a;
}

li.pull-right a {
  border-left: none;
}

.nav-pills.desktop>li:not(.pull-right) {
  display: block;
  height: 58px;
}

.nav-pills.desktop>li>a:not(.pull-right) {
  line-height: 18px;
}

.icon-align-justify {
  position: relative;
}

.icon-align-justify::before {
  /* font-family: 'Noto Sans TC', sans-serif; */
  content: '看更多';
  /* color: #000; */
  font-style: normal;
}

.icon-align-justify:hover::before {
  color: #8c001a;
}

.nav-pills .dropdown-menu {
  background: rgba(255, 255, 255, 0.9);
  padding: .6rem 1rem;
  border: 1px solid #8c001a;
  border-radius: 0;
}

.nav-pills .dropdown-menu.list {
  padding: 1.5rem 1.8rem;
}

.nav-pills .dropdown-menu li a,
.ul_inner label,
.third_list label {
  padding: .5rem .1rem;
  display: flex;
  align-items: center;
  color: #5d5d5d;
  text-decoration: none;
}

.ul_inner,
.third_list {
  border-left: 1px solid #959595;
  padding: 0 1.5rem;
}

.third_list {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding-right: 0;
}

.ul_inner label,
.third_list label {
  border-bottom: 0;
  margin-bottom: 0;
  white-space: nowrap;
}

.list .button {
  text-align: right;
}

.list .button input {
  background: #8c001a;
  padding: .6rem 1rem;
  color: #fff;
  border: none;
  border-radius: 2px;
  margin-right: 0;
  margin-top: 1rem;
}

.nav-pills .dropdown:nth-child(2) .dropdown-menu li a {
  width: 100px;
  font-size: 15px;
}

.nav-pills .dropdown:nth-child(2) .dropdown-menu li a:hover {
  background: #eee;
}

.ul_inner label,
.third_list label {
  font-size: 15px;
}

.nav-pills .dropdown-menu li:last-of-type a {
  border-bottom: none;
}

.nav-pills.desktop .dropdown-menu li a i {
  font-size: 14px;
  color: #959595;
  margin-right: .2rem;
}

.nav-pills.desktop a:hover,
.nav-pills.desktop a:hover i,
.first li.active a,
.first li.active a i {
  color: #8c001a !important;
  font-weight: bolder;
}

.nav-pills .dropdown-menu ul {
  margin-left: 0;
  margin-right: 1.5rem;
}

.open>.dropdown-menu .ul_list {
  display: flex;
}

.area_inner {
  display: flex;
  flex-wrap: wrap;
}


/* checkbox */

input[type="checkbox"] {
  display: none;
}

input[type="checkbox"]+span {
  display: inline-block;
  padding-left: 25px;
  line-height: 20px;
  background: url(../images/checkbox.png) no-repeat center left;
  user-select: none;
}

input[type="checkbox"]:checked+span {
  background: url(../images/checkbox_2.png) no-repeat center left;
  color: #8c001a;
  font-weight: bolder;
}

.festival_box {
